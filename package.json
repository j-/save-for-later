{
  "name": "save-for-later",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bun --hot src/index.ts",
    "build": "bun run scripts/build.ts",
    "postbuild": "sentry-cli sourcemaps inject dist && sentry-cli sourcemaps upload dist",
    "start": "NODE_ENV=production bun src/index.ts",
    "ladle": "bunx --bun ladle serve",
    "intl:extract": "bunx --bun formatjs extract \"src/**/*.{ts,tsx}\" --out-file messages.json",
    "intl:compile": "bunx --bun formatjs compile messages.json --out-file src/lang/en.json && rm messages.json",
    "intl": "bun run intl:extract && bun run intl:compile",
    "lint": "bunx --bun eslint src"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@fontsource/roboto": "^5.2.9",
    "@hookform/resolvers": "^5.2.2",
    "@mui/icons-material": "^7.3.7",
    "@mui/material": "^7.3.7",
    "@sentry/cli": "^2",
    "@sentry/react": "^10.32.1",
    "@tanstack/react-query": "^5.90.16",
    "@tanstack/react-query-devtools": "^5.91.2",
    "@tanstack/react-router": "^1.147.3",
    "@tanstack/react-router-devtools": "^1.149.0",
    "date-fns": "^4.1.0",
    "idb": "^8.0.3",
    "react": "^19",
    "react-dom": "^19",
    "react-hook-form": "^7.70.0",
    "react-intl": "^8.0.11",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@formatjs/cli": "^6.10.0",
    "@ladle/react": "^5.1.1",
    "@stylistic/eslint-plugin": "^5.7.0",
    "@types/bun": "latest",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9.39.2",
    "eslint-plugin-formatjs": "^6.0.10",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^17.0.0",
    "typescript-eslint": "^8.53.0"
  }
}
