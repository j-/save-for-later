    "@mui/icons-material": "^7.3.7",
    "@mui/material": "^7.3.7",
    "@tanstack/react-query": "^5.90.16",
    "date-fns": "^4.1.0",
    "idb": "^8.0.3",
    "react": "^19",
    "react-dom": "^19",
    "react-hook-form": "^7.70.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/bun": "latest"
  }
}
